{% extends "structure/base_customer.html" %}

{% block title %}Appointment Log{% endblock %}

{% block content %}
<h1>Appointment Log</h1><br>
{% if app.log == "" %}
No log for this appointment yet.
{% else %}
<pre>{{ app.log }}</pre>
{% endif %}
<br><br><a class="button big primary" href="/appointments/{{ app.id }}/">Back</a>
{% endblock %}
